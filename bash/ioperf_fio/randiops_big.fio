[global]
        bs=4K
        ioengine=libaio
        direct=1
        randrepeat=0
	size=10G
        rw=${TYPE}
        invalidate=1
        verify=0
        verify_fatal=0
        iodepth=200
        iodepth_batch=100
        iodepth_low=150
        runtime=1200
        exitall=1

[rand]
        filename=${FILENAME}
        nrfiles=${NRFILES}
        numjobs=${THREADS}
